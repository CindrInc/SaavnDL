{
  "name": "saavndl",
  "productName": "SaavnDL",
  "version": "1.0.0",
  "description": "Tool to download Saavn songs",
  "main": "app.js",
  "dependencies": {
    "download": "^7.0.0",
    "node-id3": "^0.1.5"
  },
  "devDependencies": {
    "electron": "^2.0.3"
  },
  "scripts": {
    "test": "node app.js",
    "start": "electron .",
    "package:mac": "electron-packager . --overwrite --platform=darwin --icon=../assets/mac_logo.icns --out=../builds",
    "package:win": "electron-packager . --overwrite --platform=win32 --icon=../assets/windows_logo.ico --out=../builds",
    "package:linux": "electron-packager . --overwrite --platform=linux --icon=../assets/linux_logo.png --out=../builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CindrInc/SaavnDL.git"
  },
  "author": "Cindr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CindrInc/SaavnDL/issues"
  },
  "homepage": "https://github.com/CindrInc/SaavnDL#readme"
}
